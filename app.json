{
  "expo": {
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-video"
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/LogoXscapeOkWhite.png",
          "resizeMode": "contain",
          "backgroundColor": "#85CAE4"
        }
      ],
      "expo-font"
    ],
    "name": "FRONT",
    "slug": "FRONT",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LogoXscapeOk.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/LogoXscapeOkWhite.png",
      "resizeMode": "contain",
      "backgroundColor": "#85CAE4"
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
